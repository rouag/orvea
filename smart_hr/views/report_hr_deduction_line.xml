<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<template id="report_hr_deduction_line">
			<t t-call="report.html_container">
				<t t-foreach="docs" t-as="o">
					<t t-call="report.external_layout">
						<div class="page">
							<br></br>
							<br></br>
							<br></br>

							<div class="col-xs-6">
								<strong>إسم الموظف:</strong>
								<span t-field="o.employee_id" />
								<br></br>
								<br></br>
								<strong>الرقم الوظيفي :</strong>
								<span t-field="o.number" />
								<br></br>
								<br></br>
								<strong>الوظيفة :</strong>
								<span t-field="o.job_id" />
								<br></br>
								<br></br>
								<strong>القسم :</strong>
								<span t-field="o.department_id" />
								<br></br>
								<br></br>
								<strong>القيمة :</strong>
								<span t-field="o.valuer_deduction" />
								<br></br>
								<br></br>
								<strong>نوع الحسم :</strong>
								<span t-field="o.deduction_type" />
								<br></br>
								<br></br>

							</div>
							<div class="col-xs-6">

								<strong>قسط الحسم :</strong>
								<span t-field="o.premium_dedection" />
								<br></br>
								<br></br>
								<strong>طريقة الحسم :</strong>
								<span t-field="o.deduction_way" />

								<strong>تاريخ من :</strong>
								<span t-field="o.date_from" />
								<br></br>
								<br></br>
								<strong>إلي :</strong>
								<span t-field="o.date_to" />
								<br></br>
								<br></br>
								<strong>حسمية دائمة :</strong>
								<span t-field="o.deduction_fixe" />
								<br></br>
								<br></br>
								<strong>الإجراءات :</strong>
								<span t-field="o.action_deduction" />
								<br></br>
								<br></br>
							</div>

						</div>
					</t>
				</t>
			</t>
		</template>
	</data>
</openerp>
							