<?xml version="1.0" encoding="utf-8" ?>
<openerp>
	<data>

		<!-- Leave Types -->
		<record id="data_hr_holiday_status_normal" model="hr.holidays.status">
			<field name="name">عادية</field>
			<field name="minimum" eval="5" />
			<field name="limit" eval="True" />
			<field name="postponement_period" eval="90" />
		</record>
		<record id="data_hr_holiday_status_exceptional" model="hr.holidays.status">
			<field name="name">استثنائية</field>
			<field name="limit" eval="True" />
		</record>
		<record id="data_hr_holiday_status_illness" model="hr.holidays.status">
			<field name="name">مرضية</field>
			<field name="limit" eval="True" />
		</record>
		<record id="data_hr_holiday_status_study" model="hr.holidays.status">
			<field name="name">دراسية</field>
			<field name="limit" eval="False" />
		</record>
		<record id="data_hr_holiday_status_exam" model="hr.holidays.status">
			<field name="name">إداء إمتحان</field>
			<field name="limit" eval="True" />
		</record>
		<record id="data_hr_holiday_status_patient_accompanied" model="hr.holidays.status">
			<field name="name">مرافقة مريض</field>
			<field name="limit" eval="True" />
		</record>
		<record id="data_hr_holiday_status_legal_absent" model="hr.holidays.status">
			<field name="name">غياب بعذر</field>
			<field name="limit" eval="True" />
		</record>
		<record id="data_hr_holiday_status_compensation" model="hr.holidays.status">
			<field name="name">تعويض</field>
			<field name="limit" eval="True" />
		</record>
		<record id="data_hr_holiday_status_sport" model="hr.holidays.status">
			<field name="name">الرياضية</field>
			<field name="limit" eval="True" />
		</record>
		<record id="data_hr_holiday_status_dialysis" model="hr.holidays.status">
			<field name="name">غسيل كلوي</field>
			<field name="limit" eval="True" />
		</record>
		<record id="data_hr_holiday_status_compelling" model="hr.holidays.status">
			<field name="name">اضطرارية</field>
			<field name="maximum" eval="5" />
			<field name="limit" eval="True" />
		</record>

	</data>
</openerp>