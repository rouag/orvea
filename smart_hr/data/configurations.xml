<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data noupdate="1">

		<!--أنواع جهات التوظيف -->
		<record id="governmental" model="recruiter.recruiter">
			<field name="name">حكومي</field>
		</record>

		<record id="private_company" model="recruiter.recruiter">
			<field name="name">شركة خاصة</field>
		</record>

		<!-- إعدادات نتائج تقييم موظف -->
		<record id="assessment_good" model="hr.assessment.result.config">
			<field name="name">جيد</field>
		</record>

		<record id="assessment_medium" model="hr.assessment.result.config">
			<field name="name">متوسط</field>
		</record>

		<record id="assessment_bad" model="hr.assessment.result.config">
			<field name="name">سيئ</field>
		</record>


		<!-- إعدادات مستويات التعليم -->
		<record id="certificate_primary_education" model="hr.employee.education.level">
			<field name="sequence" eval="40" />
			<field name="name">شهادة تعليم إبتدائي</field>
		</record>
		<record id="certificate_secondary_education" model="hr.employee.education.level">
			<field name="sequence" eval="50" />
			<field name="name">شهادة تعليم ثانوي</field>
		</record>
		<record id="certificate_ba" model="hr.employee.education.level">
			<field name="sequence" eval="60" />
			<field name="name">شهادة بكالوريوس</field>
		</record>
		<record id="certificate_ma" model="hr.employee.education.level">
			<field name="sequence" eval="70" />
			<field name="name">شهادة ماجستير</field>
		</record>
		<record id="certificate_engineer" model="hr.employee.education.level">
			<field name="sequence" eval="80" />
			<field name="name">شهادة مهندس</field>
		</record>
		<record id="certificate_mba" model="hr.employee.education.level">
			<field name="sequence" eval="90" />
			<field name="name">شهادة ماجستير في إدارة الأعمال</field>
		</record>
		<record id="certificate_doctorate" model="hr.employee.education.level">
			<field name="sequence" eval="100" />
			<field name="name">شهادة دكتوراه</field>
		</record>

		<!-- أنواع فئة الاستحقاق -->
		<record id="data_hr_holiday_accompaniment_exceptional" model="hr.holidays.status">
			<field name="name">اجازة مرافقة استثنائية</field>
			<field name="code">accompaniment</field>
		</record>
		<record id="data_hr_holiday_entitlement_types_parent" model="hr.holidays.entitlement.config">
			<field name="name">‫ وفاةأصل(‫أبناء‬ ‫أو‬ ‫والدين ‫أو‬ ز وجة)  </field>

			<field name="code">death</field>
		</record>
		<record id="data_hr_holiday_entitlement_types_branch" model="hr.holidays.entitlement.config">
			<field name="name">وفاة فرع (‫أحد الإخوة او غيرهم من الأقارب)</field>
			<field name="code">death</field>
		</record>
		<record id="data_hr_holiday_entitlement_types_husband_death"
			model="hr.holidays.entitlement.config">
			<field name="name"> وفاة:عدّة</field>
			<field name="code">death</field>
		</record>
		<record id="data_entitlement_illness_normal" model="hr.holidays.entitlement.config">
			<field name="name">مرضية (عادية)</field>
			<field name="code">illness</field>
		</record>
		<record id="data_entitlement_illness_serious" model="hr.holidays.entitlement.config">
			<field name="name">مرضية (مرض خطير)</field>
			<field name="code">illness</field>
		</record>

		<record id="data_entitlement_illness_work_accident" model="hr.holidays.entitlement.config">
			<field name="name">مرضية (اصابة عمل)</field>
			<field name="code">illness</field>
		</record>
		<record id="data_hr_holiday_entitlement_all" model="hr.holidays.entitlement.config">
			<field name="name"> الكل</field>
			<field name="code">all</field>
		</record>


	</data>
</openerp>
