<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data noupdate="1">

		<record id="seq_employee_medical_exam" model="ir.sequence">
			<field name="name">رقم الفحص الطبي لموظف معين</field>
			<field name="code">seq.employee.medical.exam</field>
			<field name="prefix">EME/</field>
			<field name="padding">3</field>
		</record>

		<record id="seq_judicial_precedents_ordre" model="ir.sequence">
			<field name="name">رقم طلب إستسفار</field>
			<field name="code">seq.employee.judicial.precedent.ordre</field>
			<field name="prefix">EJP/</field>
			<field name="padding">3</field>
		</record>

		<record id="hr_overtime_seq" forcecreate="1" model="ir.sequence">
			<field name="name">Overtime Decision</field>
			<field name="code">hr.overtime.seq</field>
			<field eval="1" name="number_increment" />
			<field name="padding">5</field>

		</record>

		<record id="hr_leave_seq" forcecreate="1" model="ir.sequence">
			<field name="name">Leave Request</field>
			<field name="code">hr.leave.seq</field>
			<field eval="1" name="number_increment" />
			<field name="padding">5</field>
		</record>
		
		<record id="hr_leave_cancellation_seq" forcecreate="1" model="ir.sequence">
            <field name="name">Leave Cancellation</field>
            <field name="code">hr.leave.cancellation.seq</field>
            <field eval="1" name="number_increment"/>
            <field name="padding">5</field>
        </record>

	</data>
</openerp>
